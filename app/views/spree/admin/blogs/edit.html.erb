<% content_for :page_title do %>
  <%= link_to Spree.t(:blog), admin_blogs_url %> /
  <%= @blog.title %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages',
  locals: { target: @blog } %>

<%= form_for [:admin, @blog] do |f| %>
  <%= render partial: "form", locals: {f: f} %>
<% end %>
