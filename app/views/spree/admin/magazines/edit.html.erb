<% content_for :page_title do %>
  <%= link_to Spree.t(:magazines), admin_magazines_url %> /
  <%= @magazine.title %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages',
  locals: { target: @magazine } %>

<%= form_for [:admin, @magazine] do |f| %>
  <%= render partial: "form", locals: {f: f} %>
<% end %>
